{
  "expo": {
    "name": "picklist",
    "slug": "picklist",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.araitatsuyacode.picklist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "商品画像の選択のためにフォトライブラリへアクセスします。",
        "NSCameraUsageDescription": "商品の写真撮影のためにカメラを使用します。 (To take product photos)"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "picklist",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a1b17b17-9770-4433-9643-81247aa08e20"
      }
    }
  }
}